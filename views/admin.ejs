<%- include("partials/header") %>

<h2>Admin felulet</h2>
<p>Felhasznalok listaja.</p>

<div class="table-responsive">
  <table class="table table-striped table-hover align-middle">
    <thead class="table-dark">
      <tr>
        <th>ID</th>
        <th>Nev</th>
        <th>Email</th>
        <th>Szerep</th>
      </tr>
    </thead>
    <tbody>
    <% users.forEach(function(u){ %>
      <tr>
        <td><%= u.id %></td>
        <td><%= u.nev %></td>
        <td><%= u.email %></td>
        <td><%= u.szerep %></td>
      </tr>
    <% }) %>
    </tbody>
  </table>
</div>

<%- include("partials/footer") %>
